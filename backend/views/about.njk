{% extends "layout.njk" %}

{% block head %}

<title>Cursos</title>

{% endblock %}

{% block content %}

<div id="wrapper">
    <img src="{{ about.avatar_url }}"
        alt="Imagem de {{ about.name }}">
    <h1>{{ about.name }}</h1>
    <h2>{{ about.role }}</h2>
    <p>{{ about.description }}</p>
</div>

{% endblock %}